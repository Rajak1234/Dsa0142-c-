#include<iostream>
using namespace std;
int main()
{
	int a,b,c,d,e,f,g;
	a=5;
	b=6;
	c=a+b;
	d=a-b;
	e=a*b;
	f=a/b;
	g=a%b;
	cout << "\n addition is \n" << c;
    cout << "\n subtraction is \n" << d;
    cout << "\n multiplication is \n" << e;
	cout << "\n division is \n" << f;
	cout << "\n modulus is \n" << g;	
	return 0;			
}
